                  },
                  "behaviours": [
                    {
                      "intent": "*",
                      "required": "true",
                      "hide": {
                        "hideWhenExpression": "infantStatus === '125872AAAAAAAAAAAAAAAAAAAAAAAAAAAAAA'"
                      },
                      "validators": [
                        {
                          "type": "js_expression",
                          "failsWhenExpression": "!/^[0-9]{5}[A-Z]{1}[0-9]{7,8}$$/.test(myValue)",
