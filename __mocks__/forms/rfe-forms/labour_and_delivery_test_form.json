                    "concept": "164803AAAAAAAAAAAAAAAAAAAAAAAAAAAAAA",
                    "rendering": "text",
                    "calculate": {
                      "calculateExpression": "!isEmpty('MotherPtracker_id') ? myValue = customGenerateInfantPTrackerId(node.value.id, MotherPtracker_id) : ''"
